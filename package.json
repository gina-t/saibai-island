{
  "name": "saibai-island",
  "version": "1.0.0",
  "description": "Design solution for temporary flood mitigation structures Saibai Island",
  "homepage": "https://github.com/gina-t/saibai-island#readme",
  "bugs": {
    "url": "https://github.com/gina-t/saibai-island/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gina-t/saibai-island.git"
  },
  "license": "MIT",
  "author": "ginadrcoder@gmail.com",
  "type": "module",
  "main": "server/dist/server.js",
  "scripts": {
    "start": "node server/dist/server.js",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "cd client && npm run dev",
    "dev:server": "cd server && npm run dev",
    "build": "concurrently \"npm run build:client\" \"npm run build:server\"",
    "build:client": "cd client && npm run build",
    "build:server": "cd server && npm run build",
    "render-build": "npm install --include=dev && cd server && npm install --include=dev && cd ../client && npm install --include=dev && cd .. && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "concurrently": "^9.2.1"
  }
}
